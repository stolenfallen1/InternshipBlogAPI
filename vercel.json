{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/posts",
            "methods": [
                "POST"
            ],
            "dest": "/server.js"
        },
        {
            "src": "/get/:postId?",
            "methods": [
                "GET"
            ],
            "dest": "/server.js"
        }
    ]
}