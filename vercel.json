{
    "version": 2,
    "builds": [
        {
            "src": "Api/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/posts",
            "dest": "Api/server.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/get/:postId?",
            "dest": "Api/server.js",
            "methods": [
                "GET"
            ]
        }
    ]
}